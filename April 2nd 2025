class Solution:
    def maximumTripletValue(self, nums: List[int]) -> int:
        c=0
        for i in range(len(nums)-2):
            j=i+1
            while j<len(nums) -1:
                k=j+1
                while k<len(nums):
                    c=max(c,(nums[i]-nums[j])*nums[k])
                    k+=1
                j+=1
        return c
